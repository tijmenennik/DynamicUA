{
  "name": "DynamicUA",
  "version": "1.4",
  "description": "Faking the user-agent for websites that pretend to require Chrome.",
  "permissions": [
    "cookies",
    "webRequest",
    "tabs",
    "notifications",
    "webRequestBlocking",
    "*://*.google.com/",     
    "*://*.youtube.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "page_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
